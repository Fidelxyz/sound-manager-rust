{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "platforms": [
    "macOS",
    "windows",
    "linux"
  ],
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging",
    "dialog:allow-open",
    "store:allow-load",
    "store:allow-get",
    "store:allow-set",
    "window-state:allow-save-window-state",
    "os:allow-platform"
  ]
}
